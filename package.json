{
  "name": "webc",
  "displayName": "webc",
  "description": "VSCode Extension for WebC files",
  "homepage": "https://github.com/dwkns/vscode-webc/",
  "repository": {
    "url": "https://github.com/dwkns/vscode-webc/"
  },
  "publisher": "dwkns",
  "license": "MIT",
  "author": {
    "name": "dwkns",
    "email": "dwkns@me.com",
    "url": "https://dwkns.com"
  },
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.73.0"
  },
  "scripts": {
    "pub": "npm version patch && vsce package && vsce publish"
  },
  "icon": "images/logo.webp",
  "contributes": {
    "themes": [
      {
        "uiTheme": "vs-dark",
        "label": "webc-dark",
        "path": "./themes/webc-dark.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/webc.injection.json",
        "scopeName": "source.webc",
        "injectTo": [
          "text.html.derivative"
        ]
      }
    ]
  }
}
