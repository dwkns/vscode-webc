{
  "name": "webc",
  "displayName": "webc",
  "description": "VSCode Extension for WebC files",
  "repository": {
    "url": "https://github.com/dwkns/vscode-webc.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "contributes": {
    "themes": [
      {
        "uiTheme": "vs-dark",
        "label": "webc-dark",
        "path": "./themes/webc-dark.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/webc.injection.json",
        "scopeName": "webc.injection",
        "injectTo": ["text.html.derivative"]
      }
    ]
  }
}
