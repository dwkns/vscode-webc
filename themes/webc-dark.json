{
    "name": "webc-dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "customLiteral": "#D4D4D4",
        "newOperator": "#d4d4d4",
        "numberLiteral": "#b5cea8",
        "stringLiteral": "#ce9178"
    },
    "tokenColors": [
      // debug
      {
        "name": "Todo",
        "scope": ["todo"],
        "settings": {
          "foreground": "#b80000"
        }
      },
      // JS
      {
        "name": "js/ts string",
        "scope": "string.js, string.quoted.single.js",
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "name": "js/ts entity.name.function.js",
        "scope": "entity.name.function.js,meta.function-call.js",
        "settings": {
          "foreground": "#ABB2BF"
        }
      },

      // general
      {
        "name": "string",
        "scope": "string",
        "settings": {
          "foreground": "#98C379"
        }
      },

      {
        "name": "Invalid",
        "scope": ["invalid", "invalid.illegal"],
        "settings": {
          "foreground": "#FF5370"
        }
      },

      {
        "name": "Regular Expressions",
        "scope": ["string.regexp"],
        "settings": {
          "foreground": "#ffe600"
        }
      },
      {
        "name": "Escape Characters",
        "scope": ["constant.character.escape"],
        "settings": {
          "foreground": "#ffe600"
        }
      },

      {
        "name": "Base color",
        "scope": ["text.html.derivative", "text.html.njk"],
        "settings": {
          "foreground": "#DEECEC"
        }
      },

      {
        "name": "storage.type",
        "scope": "storage.type",
        "settings": {
          "foreground": "#56B6C2"
        }
      },
      {
        "name": "variable.other.constant",
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#61AFEF"
        }
      },
      {
        "name": "keyword.operator",
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#E06C75"
        }
      },
      {
        "name": "entity.name.function",
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#98C379"
        }
      },
      {
        "name": "meta.brace",
        "scope": "meta.brace",
        "settings": {
          "foreground": "#ABB2BF"
        }
      },

      {
        "name": "variable.parameter",
        "scope": "variable.parameter",
        "settings": { "foreground": "#D19A66", "fontStyle": "italic" }
      },
      {
        "name": "variable.other",
        "scope": "variable.other",
        "settings": { "foreground": "#ABB2BF", "fontStyle": "italic" }
      },
      {
        "name": "variable.other.object",
        "scope": "variable.other.object",
        "settings": { "foreground": "#61AFEF", "fontStyle": "italic" }
      },
      {
        "name": "comment",
        "scope": "comment",
        "settings": { "foreground": "#717272", "fontStyle": "italic" }
      },
      {
        "name": "constant.numeric",
        "scope": "constant.numeric",
        "settings": { "foreground": "#C678DD" }
      },
      {
        "name": "punctuation.definition.template-expression",
        "scope": "punctuation.definition.template-expression",
        "settings": { "foreground": "#C678DD" }
      },
      {
        "name": "keyword",
        "scope": "keyword",
        "settings": { "foreground": "#E06C75" }
      },
      {
        "name": "support.type.object.module.js",
        "scope": "support.type.object.module.js",
        "settings": { "foreground": "#56B6C2" }
      },
      // NJK formatting
      {
        "name": "NJK | tags ",
        "scope": "punctuation.definition.tag.begin.njk, punctuation.definition.tag.end.njk, keyword.other.whitespace.njk",
        "settings": {
          "foreground": "#f06060"
        }
      },

      {
        "name": "NJK | punctuation ",
        "scope": "punctuation.output.njk, punctuation.tag.njk",
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "name": "NJK | entity name ",
        "scope": "entity.name.tag.njk, keyword.operator.njk",
        "settings": {
          "foreground": "#577dc0"
        }
      },
      {
        "name": "NJK | variable ",
        "scope": "variable.other.njk",
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "name": "NJK | variable parmater ",
        "scope": "variable.parameter.njk",
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "name": "NJK | Support Class??? Should be same as Variable ",
        "scope": "support.class.njk",
        "settings": {
          "foreground": "#E5C07B"
        }
      },

      {
        "name": "NJK | variable.parameter.njk",
        "scope": "variable.parameter.njk",
        "settings": { "foreground": "#E5C07B", "fontStyle": "italic" }
      },
      {
        "name": "NJK | variable.other.njk",
        "scope": "variable.other.njk",
        "settings": { "foreground": "#E5C07B", "fontStyle": "italic" }
      },

      {
        "name": "NJK | support.function.without-args.njk",
        "scope": "support.function.without-args.njk",
        "settings": { "foreground": "#E5C07B" }
      },

      // frontmatter
      {
        "name": "Frontmatter | start/end ",
        "scope": "meta.embedded.block.frontmatter.njk punctuation.definition.tag.end.njk, meta.embedded.block.frontmatter.njk punctuation.definition.tag.begin.njk",
        "settings": {
          "foreground": "#F06060",
          "fontStyle": "bold"
        }
      },

      //entity.name.tag.yaml
      {
        "name": "entity.name.tag.yaml",
        "scope": ["entity.name.tag.yaml"],
        "settings": {
          "foreground": "#C678DD"
        }
      },
      {
        "name": "constant.numeric.integer.yaml",
        "scope": ["constant.numeric.integer.yaml"],
        "settings": {
          "foreground": "#a87546"
        }
      },
      //HTML
      {
        "name": "entity.name.tag.html",
        "scope": ["entity.name.tag.html"],
        "settings": {
          "foreground": "#C678DD"
        }
      },
      {
        "name": "Attributes",
        "scope": [
          "entity.other.attribute-name.html",
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      //JSON
      {
        "name": "JSON | Key | Level 0",
        "scope": [
          "source.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "name": "JSON | Key | Level 1",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#FFCB6B"
        }
      },
      {
        "name": "JSON | Key | Level 2",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "name": "JSON | Key | Level 3",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#D19A66"
        }
      },
      {
        "name": "JSON | Key | Level 4",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#f07178"
        }
      },
      {
        "name": "JSON | Key | Level 5",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#82AAFF"
        }
      },
      {
        "name": "JSON | Key | Level 6",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#f07178"
        }
      },
      {
        "name": "JSON | Key | Level 7",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "name": "JSON | Key | Level 8",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      // CSS
      {
        "name": "CSS | Classes",
        "scope": ["entity.other.attribute-name.class"],
        "settings": {
          "foreground": "#b9b84f"
        }
      },
      {
        "name": "CSS | entity.name.tag.css",
        "scope": ["entity.name.tag.css"],
        "settings": {
          "foreground": "#f42424"
        }
      },
      {
        "name": "CSS | support.type.property-name.css",
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#DEECEC"
        }
      },
      {
        "name": "CSS | support.constant.property-value.css",
        "scope": [
          "support.constant.property-value.css",
          "support.constant.color.current.css"
        ],
        "settings": {
          "foreground": "#89DDFF",
          "fontStyle": "bold"
        }
      },
      {
        "name": "CSS | entity.other.attribute-name.pseudo-class.css",
        "scope": ["entity.other.attribute-name.pseudo-class.css"],
        "settings": {
          "foreground": "#89DDFF",
          "fontStyle": "italic"
        }
      },

      {
        "name": "CSS | support.function.misc.css",
        "scope": ["support.function.misc.css"],
        "settings": {
          "foreground": "#D19A66",
          "fontStyle": "italic bold"
        }
      },

      {
        "name": "CSS | punctuation.definition.constant.css",
        "scope": [
          "punctuation.definition.constant.css",
          "constant.other.color.rgb-value.hex.css"
        ],
        "settings": {
          "foreground": "#C678DD",
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "CSS | constant.numeric.css",
        "scope": ["constant.numeric.css"],
        "settings": {
          "foreground": "#C678DD",
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "CSS | keyword.other.unit",
        "scope": ["keyword.other.unit"],
        "settings": {
          "foreground": "#E06C75",
          "fontStyle": "italic bold"
        }
      }
    ],
    "type": "dark",
    "colors": {
       
    }
}